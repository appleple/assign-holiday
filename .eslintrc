{
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "browser": true
  },
  "globals": {
    "document": true,
    "window": true
  },
  "rules": {
    "comma-dangle": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "prefer-destructuring": 0,
    "no-restricted-syntax": 0,
    "max-len": 0,
    "no-new": 0,
    "no-case-declarations": 0
  }
}
